@model Department
@{
    ViewData["TitlePhrase"]=@Model.Name;
}
<h1>Detail For Employee  <a class="btn btn-outline-primary" href="/department/index" role="button">Go Back</a></h1>
 
<ul class="list-group">
<li class="list-group-item">Name: @Model.Name</li>
<li class="list-group-item">Department Code: @Model.Code</li>
<li class="list-group-item">Description: @Model.Description</li>
<li class="list-group-item">Start Date: @Model.StartDate</li>

</ul>